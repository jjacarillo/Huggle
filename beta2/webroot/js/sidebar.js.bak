  getSubscribers: function(e){
       $.ajax({url: 'subscribers',
	    data: 'GET',
            dataType: 'text',
            success: function(msg){
		    $('#rightx').html(msg);
		    App.oTable = $('#datatable').dataTable({
		            'bProcessing': true,
		            'bServerSide':true,
		            'sAjaxSource' : 'generate_list'
		    });
	    }
    });

  },
  viewHistory: function(e){
       $.ajax({url: 'view_history',
	    data: 'GET',
            dataType: 'text',
            success: function(msg){
		    $('#rightx').html(msg);
	    }
    });

  },
  changePassword: function(e){
  
    $.ajax({url: 'change_password',
	    data: 'GET',
            dataType: 'text',
            success: function(msg){
		    $('#rightx').html(msg);
	    }
    });
  },
  editProfile: function(e){
  
    $.ajax({url: 'edit_profile',
	    data: 'GET',
            dataType: 'text',
            success: function(msg){
		    $('#rightx').html(msg);
	    }
    });
  },
  createDeal: function(e){
	  
	    $.ajax({url: 'create_deal',
		    data: 'GET',
	            dataType: 'text',
	            success: function(msg){
			    $('#rightx').html(msg);
		    }
	    });
	  },
  initialize: function(){
    _.bindAll(this);
  }
});
